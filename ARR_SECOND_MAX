// Online C++ compiler to run C++ program online
#include <iostream>
#include <bits/stdc++.h>

using namespace std;

int main() {
    // Write C++ code here
    int arr[] = {3,2,-1,2,-3,-1,-5,4,-1,4,3,-2,-4,-5};
    int size = sizeof(arr)/sizeof(int);
    
    int max = arr[0];
    int sec_max = 0;
    for(int i = 0;i < size; i++) {
         if(arr[i] > max) {
            sec_max = max;
            max = arr[i];
         }
         else if(arr[i] > sec_max && arr[i] != max)
            sec_max = arr[i];
    }
    printf("Second Max Value is %d ", sec_max);
    return 0;
}
