#include <stdio.h>

int main()
{
    int arr[] = {1,3,4,2,2};
    int size = sizeof(arr)/sizeof(int);
    int sum = ((size-1)*(size))/2;
    int arr_sum = 0;
    for(int i = 0; i <= size -1; i++) {
        arr_sum += arr[i];
    }    
    printf("Repeated number is %d", arr_sum-sum);
    return 0;
}
